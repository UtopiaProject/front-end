{
  "name": "utopia-front-end",
  "version": "0.1.0",
  "description": "React front-end for the Utopia project",
  "main": "./src/index.js",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --watch",
    "test:coverage": "nyc npm test",
    "coveralls": "npm run test:coverage && nyc report --reporter=text-lcov | coveralls",
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint"
  },
  "nyc": {
    "functions": 80,
    "lines": 80,
    "check-coverage": true,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UtopiaProject/front-end.git"
  },
  "keywords": [
    "utopia",
    "tcc",
    "front-end"
  ],
  "author": "Filipe Gorges Reuwsaat <lipirr@hotmail.com> (https://github.com/filipegorges)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UtopiaProject/front-end/issues"
  },
  "homepage": "https://github.com/UtopiaProject/front-end#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.1",
    "coveralls": "^3.0.2",
    "enzyme": "^3.5.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-react": "^7.11.0",
    "husky": "^0.14.3",
    "jsdom": "^12.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "node-fetch": "^2.2.0",
    "nyc": "^13.0.1",
    "sinon": "^6.1.5",
    "sinon-chai": "^3.2.0"
  }
}
